{
  "packages": [
    "python@3.13.3",
    "python313Packages.jupyterlab@4.4.3",
    "python313Packages.bash-kernel@0.10.0",
    "jq@1.8.1",
    "vault-bin@1.20.0",
    "terraform@1.12.2",
    "openssl@3.5.1",
    "kind@0.29.0",
    "kubectl@1.33.3",
    "docker-client@28.3.3",
    "yq-go@4.47.1",
    "gnupg@2.4.8",
    "postgresql@17.5",
    "openldap@2.6.9",
  ],
  "shell": {
    "init_hook": [
      "mkdir -p /tmp/vault",
      "python -m bash_kernel.install --user",
    ],
    "scripts": {
      "start": [
        "jupyter lab --ip=0.0.0.0 --port=8888 --no-browser --allow-root --NotebookApp.token=''",
      ],
      "stop": [
        "pkill jupyter",
        "pkill -x vault",
      ],
    },
  },
}
